<%- if controller_name != 'sessions' %>
  <%= link_to "ログインはこちら", new_session_path(resource_name), class: "text-white" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "無料で新規登録", new_registration_path(resource_name), class: "text-white" %><br />
<% end %>

